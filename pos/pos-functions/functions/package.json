{
  "name": "pos-functions",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run build && firebase use default && cross-env GOOGLE_APPLICATION_CREDENTIALS=\"./service-account.json\" firebase emulators:start --only functions --inspect-functions",
    "dev:all": "npm run build && GOOGLE_APPLICATION_CREDENTIALS=\"./service-account.json\" firebase emulators:start",
    "dev:old": "npm run build:watch | firebase emulators:start --only functions --inspect-functions",
    "dev:ts-node": "ts-node ./src/index.ts && firebase emulators:start",
    "lint": "eslint --ext .js,.ts --fix .",
    "format": "prettier --write .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "serve:all": "npm run build && firebase emulators:start",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npm run build && firebase deploy --only functions --project default",
    "logs": "firebase functions:log",
    "script-get-data": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"./service-account.json\" ts-node ./src/scripts/get-data.ts",
    "script-seed-data": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"./service-account.json\" ts-node ./src/scripts/seed-accounting.ts"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.2.0",
    "@node-rs/bcrypt": "^1.10.7",
    "@pos/shared-models": "workspace:^",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.6",
    "dotenv": "16.4.7",
    "express": "^4.21.2",
    "firebase-admin": "^10.3.0",
    "firebase-functions": "5.1.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.34",
    "nodemailer": "^6.7.5",
    "random-string-alphanumeric-generator": "^0.12.2",
    "randomstring": "^1.3.0",
    "ua-parser-js": "^2.0.3",
    "ulid": "^3.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.21",
    "@types/express-serve-static-core": "4.17.30",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/nodemailer": "^6.4.10",
    "@types/randomstring": "^1.1.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
  },
  "private": true
}
